<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="fill_parent"    android:layout_height="wrap_content"    android:orientation="horizontal"    android:background="@color/white"    android:padding="5dip"    >  <TextView   android:layout_height="wrap_content"   android:layout_width="80dp"   android:id="@+id/contact_name"   android:textColor="@color/black"   android:layout_alignParentLeft="true"   android:layout_alignParentTop="true"   android:layout_marginTop="5dp"   android:layout_marginLeft="10dp"   android:gravity="right"   android:text="Name:"   ></TextView>    <TextView   android:layout_height="wrap_content"   android:layout_width="80dp"   android:id="@+id/contact_ext"   android:textColor="@color/black"   android:layout_below="@+id/contact_name"   android:layout_alignLeft="@+id/contact_name"   android:gravity="right"   android:text="Ext:">      </TextView>   <TextView   android:layout_height="wrap_content"   android:layout_width="80dp"   android:id="@+id/cellphone"   android:textColor="@color/black"   android:layout_below="@+id/contact_ext"   android:layout_alignLeft="@+id/contact_ext"   android:gravity="right"   android:text="Cellphone:">      </TextView>   <TextView   android:layout_height="wrap_content"   android:layout_width="80dp"   android:id="@+id/email"   android:textColor="@color/black"   android:layout_below="@+id/cellphone"   android:layout_alignLeft="@+id/cellphone"   android:gravity="right"   android:text="Email:">      </TextView>  <TextView      android:id="@+id/ta_oce01"      android:layout_width="wrap_content"      android:layout_height="wrap_content"      android:layout_alignParentTop="true"      android:layout_marginTop="5dp"      android:layout_marginLeft="10dp"      android:layout_toRightOf="@+id/contact_name"      android:textColor="@color/black"      android:text="" >       </TextView>   <TextView    android:id="@+id/ta_oce02"   android:layout_below="@+id/ta_oce01"   android:layout_alignLeft="@+id/ta_oce01"   android:layout_width="wrap_content"   android:layout_height="wrap_content"   android:text=""   android:textColor="@color/black"   >  </TextView>    <TextView      android:id="@+id/oce07"      android:layout_width="wrap_content"      android:layout_height="wrap_content"      android:layout_below="@+id/ta_oce02"      android:layout_alignLeft="@+id/ta_oce02"      android:layout_toRightOf="@+id/contact_name"      android:textColor="@color/black"      android:text="" >       </TextView>   <TextView    android:id="@+id/oce05"   android:layout_below="@+id/oce07"   android:layout_alignLeft="@+id/oce07"   android:layout_width="wrap_content"   android:layout_height="wrap_content"   android:text=""   android:textColor="@color/black"   >  </TextView>     <View          android:id="@+id/bottm_line"          android:layout_width="fill_parent"          android:layout_height="1dp"          android:layout_alignParentLeft="true"          android:layout_marginTop="10dp"          android:layout_marginLeft="10dp"          android:layout_marginRight="10dp"          android:layout_below="@+id/oce05"          android:background="@color/red"          /></RelativeLayout>