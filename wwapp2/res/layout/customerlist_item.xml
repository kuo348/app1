<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="0dip"
     android:descendantFocusability="blocksDescendants"
    >
    <RelativeLayout
        android:id="@+id/customer_icon_layout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:padding="0dp" >
        <ImageView
            android:id="@+id/customer_icon"
            android:layout_width="42dp"
            android:layout_height="42dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/customer_list_icon_1" />
        <TextView
            android:id="@+id/lb_customer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="30dp"
            android:layout_marginTop="20dp"
            android:layout_toRightOf="@+id/customer_icon"
              android:background="@color/transparent"
            android:text="0000 WinWay"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lb_sales"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="40dp"
            android:text="terisa lin"
            android:gravity="center|left"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:layout_toRightOf="@+id/sales_icon"
            android:layout_alignBottom="@+id/sales_icon"
            android:layout_alignTop="@+id/sales_icon"
            android:typeface="sans" />

        <ImageView
            android:id="@+id/sales_icon"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="10dp"
            android:layout_alignLeft="@+id/lb_customer"
            android:layout_below="@+id/customer_icon"
            android:src="@drawable/customer_list_icon_6" />
           <TextView
            android:id="@+id/lb_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="40dp"
            android:layout_alignTop="@+id/email_icon"
            android:layout_below="@+id/lb_sales"
            android:text="07 36109999"
              android:background="@color/transparent"
            android:gravity="center|left"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:layout_toRightOf="@+id/email_icon"
            android:layout_alignBottom="@+id/email_icon"
            android:typeface="sans" />

        <ImageView
            android:id="@+id/email_icon"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="10dp"
            android:layout_alignLeft="@+id/lb_customer"
            android:layout_below="@+id/sales_icon"
            android:src="@drawable/customer_list_email_1" />
        
          <TextView
            android:id="@+id/lb_tel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="40dp"
            android:layout_alignTop="@+id/tel_icon"
            android:layout_below="@+id/lb_email"
            android:text="07 36109999"
            android:gravity="center|left"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:background="@color/transparent"
            android:layout_toRightOf="@+id/tel_icon"
            android:layout_alignBottom="@+id/tel_icon"
            android:typeface="sans" />

        <ImageView
            android:id="@+id/tel_icon"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="10dp"
            android:layout_alignLeft="@+id/lb_customer"
            android:layout_below="@+id/email_icon"
            android:src="@drawable/customer_list_tel_1" />
         <ImageView
            android:id="@+id/level_icon"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginRight="5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@color/transparent"
            android:src="@drawable/customer_level_b_6" />

  <View
      android:layout_width="match_parent"
      android:layout_height="10dp"
      android:layout_below="@+id/tel_icon" />
</RelativeLayout>
</RelativeLayout>
