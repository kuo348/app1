<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@android:color/white"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:orientation="vertical"
         android:layout_width="fill_parent"
         android:layout_height="fill_parent"
         android:divider="@color/list_divider" 
         android:showDividers="end">
        <RelativeLayout android:id="@+id/cust_r1" 
            android:background="@drawable/actionbar_bg"
             android:layout_width="match_parent"
             android:layout_height="44.0dip" 
             android:layout_alignParentTop="true">
            <TextView android:textAppearance="?android:textAppearanceMedium" 
                android:textColor="@color/white" 
                android:id="@+id/detail_title_text" 
                android:layout_width="wrap_content" 
                android:layout_height="wrap_content" 
                android:text="QT"
                android:layout_centerInParent="true" />
            <Button
                android:textColor="@color/white"
                android:id="@+id/btnAddQt"
                android:background="@color/transparent" 
                android:layout_width="wrap_content" 
                android:layout_height="wrap_content" 
                android:layout_marginRight="10.0dip" 
                android:text="" 
                android:drawableRight="@drawable/ic_action_new" 
                android:layout_alignParentTop="true" 
                android:layout_alignParentRight="true" 
                android:layout_centerVertical="true"
                android:enabled="true"
                android:clickable="true"
                />
            <!--android:onClick="createQT"-->

            <!--android:onClick="actqtlist_btnAddQt_onClick"-->
            <!--android:enabled="false"-->

            <Button android:textColor="@color/white"
                android:id="@+id/btnReturnSale" 
                android:background="@color/transparent" 
                android:layout_width="wrap_content" 
                android:layout_height="wrap_content"
                 android:layout_marginLeft="10.0dip"
                  android:text="Sale" 
                  android:drawableLeft="@drawable/group_arrow_left" 
                  android:layout_alignParentLeft="true" 
                  android:layout_alignParentTop="true" 
                  />
        </RelativeLayout>

        <RelativeLayout android:id="@+id/cust_r2"
            android:background="@drawable/actionbar_bg" 
            android:layout_width="match_parent"
            android:layout_height="50.0dip" 
            android:layout_alignParentTop="true">

            <TableLayout
                android:id="@+id/R01_M12_R_rel_tbl"
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_toLeftOf="@+id/cmd_r1_x"
                android:background="@color/transparent"
                >

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="50.0dip"
                    >
                    <RelativeLayout
                        android:id="@+id/cust_r2_z"
                        android:layout_width="fill_parent"
                        android:layout_height="50.0dip"
                        android:background="@color/transparent"
                        android:layout_weight="100"
                        >
                        <!--android:layout_toLeftOf="@+id/cmd_r1"-->

                        <EditText
                            android:id="@+id/search_text"
                            android:layout_width="fill_parent"
                            android:layout_height="30.0dp"
                            android:textAppearance="?android:textAppearanceMedium"
                            android:background="@color/white"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:layout_marginLeft="10.0dp"
                            android:layout_marginRight="10.0dp"
                            android:text=""
                            android:hint="search"
                            android:gravity="left|center_vertical"
                            android:singleLine="true"
                            android:drawableLeft="@drawable/ic_action_search"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:inputType="text"
                            android:imeOptions="actionSearch"
                            android:layout_weight="100"
                            />
                        <!--android:drawableRight="@drawable/stock_search_send3"-->
                        <Button
                            android:id="@+id/btn_basic_search"
                            android:textColor="@color/white"
                            android:background="@drawable/stock_search_send3"
                            android:layout_width="33.0dip"
                            android:layout_height="33.0dip"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="12dp"
                            android:layout_marginLeft="10.0dp"
                            android:layout_marginRight="10.0dp"
                            android:text=""
                            android:layout_alignParentRight="true"
                            android:gravity="left|center_vertical"
                            android:visibility="gone"
                            />
                        <!--android:scaleType="fitXY"-->
                        <!--android:layout_rightInParent="true"-->
                        <!--android:layout_centerInParent="true"-->
                        <!--android:layout_toLeftOf="@+id/cmd_r1"-->

                    </RelativeLayout>

                    <!--@drawable/stock_search_send2-->

                </TableRow>


            </TableLayout>


             <RelativeLayout
                 android:id="@+id/cmd_r1_x"
                 android:layout_width="60dp"
                 android:layout_height="fill_parent"
                 android:layout_toLeftOf="@+id/cmd_r1"
                 android:visibility="gone"
                 >

                 <Button
                     android:id="@+id/btn_basic_search_x"
                     android:textColor="@color/white"
                     android:background="@drawable/apqp_search_advance"
                     android:layout_width="33.0dip"
                     android:layout_height="25.0dip"
                     android:layout_marginRight="10.0dip"
                     android:scaleType="fitXY"
                     android:text=""
                     android:layout_centerInParent="true"
                     android:visibility="gone"
                     />
             </RelativeLayout>

            <RelativeLayout
                android:id="@+id/cmd_r1"
                android:layout_width="60dp"
                android:layout_height="fill_parent"
                android:layout_alignParentRight="true" >

                <Button
                    android:id="@+id/btn_adv_search"
                    android:textColor="@color/white"
                    android:background="@drawable/apqp_search_advance"
                    android:layout_width="33.0dip"
                    android:layout_height="25.0dip"
                    android:layout_marginRight="10.0dip"
                    android:scaleType="fitXY"
                    android:text=""
                    android:layout_centerInParent="true"
                    />
                <Button android:textAppearance="?android:textAppearanceMedium"
                    android:textColor="@color/white"
                    android:id="@+id/btn_cancel"
                    android:background="@color/transparent"
                    android:visibility="gone"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:text="Cancel"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>

        </RelativeLayout>
        <RelativeLayout
            android:visibility="gone"
            android:id="@+id/cust_r3"
            android:layout_below="@+id/cust_r2"
            android:background="@drawable/actionbar_bg"
            android:layout_width="match_parent"
            android:layout_height="40.0dip"
            android:layout_alignParentTop="true">
            <RadioGroup
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"        
        android:id="@+id/radiogroup1"     
        android:background="@drawable/apqp_radiogroup_divider"   
        android:layout_width="fill_parent"
        android:layout_height="30dp"      
             android:orientation="horizontal" >
        <RadioButton
            android:id="@+id/tabQt"
            style="@style/navbar_button"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"         
            android:gravity="center"           
            android:text="QTNO"
            android:textColor="@color/red"
            android:checked="true"
            android:divider="@drawable/apqp_radiogroup_divider"
            android:background="@drawable/apqp_radiogroup_select"
            android:textSize="14dp" />
        <RadioButton
            android:id="@+id/tabCust"
            style="@style/navbar_button"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"         
            android:text="Customer"
            android:divider="@drawable/apqp_radiogroup_divider"
            android:background="@drawable/apqp_radiogroup_select"
            android:textColor="@color/white"
            android:textSize="14dp" />
              </RadioGroup>
            
        </RelativeLayout>
        <com.winway.wwapp2.PullDownView
            android:id="@+id/qtlistview"
            android:padding="0.0dip"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" />
    </LinearLayout>
</RelativeLayout>
