<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="80dp"     android:background="@color/white">    <TextView        android:id="@+id/msg_title2"        android:layout_width="wrap_content"        android:layout_height="match_parent"        android:gravity="center_vertical"        android:text="123456"        android:minHeight="?android:attr/listPreferredItemHeightSmall"        android:textAppearance="?android:attr/textAppearanceListItemSmall"        android:textColor="@color/black" />  <TextView        android:id="@+id/msg_content"        android:layout_width="wrap_content"        android:layout_height="match_parent"        android:gravity="center_vertical"              android:text="123456"        android:visibility="gone"        android:textColor="@color/black" />    <ImageView        android:id="@+id/icon"        android:layout_width="25dp"        android:layout_height="wrap_content"        android:layout_alignParentBottom="true"        android:layout_alignParentRight="true"        android:layout_alignParentTop="true"        android:contentDescription="@string/desc_list_item_icon"        android:src="@drawable/group_arrow_right" /></RelativeLayout>