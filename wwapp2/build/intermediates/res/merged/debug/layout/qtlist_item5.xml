<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:background="@drawable/list_selector"
    android:padding="5dip"
    android:descendantFocusability="blocksDescendants"
    >

    <LinearLayout
        android:id="@+id/ln_05"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        >

        <!-- tb1 begin -->
        <ImageView
            android:id="@+id/cust_detail_top_icon"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"
            android:src="@drawable/customer_detail_top_1"
            />

        <!-- detail_tb1 -->
        <TableLayout
            android:id="@+id/detail_tb5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/cust_detail_top_icon"
            android:background="@drawable/customer_detail_body_1"
            android:divider="@drawable/customer_detail_line_1"
            android:stretchColumns="2"
            android:showDividers="middle"
            >

            <!-- QT_TYPE -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_M11_QT_TYPE"
                    android:text="QT_TYPE"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_M11_QT_TYPE"
                    android:text="QT"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

            <!-- QT_NO -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_M11_QT_NO"
                    android:text="QT_NO"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_M11_QT_NO"
                    android:text="1234567890"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

            <!-- QT_SEQ -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_M11_QT_SEQ"
                    android:text="Item#"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_M11_QT_SEQ"
                    android:text="0001"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

            <!-- QTY1 -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_QTY1"
                    android:text="QTY1"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_QTY1"
                    android:text="2"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

            <!-- QTY2 -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_QTY2"
                    android:text="QTY2"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_QTY2"
                    android:text="5"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

            <!-- UNIT -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_UNIT"
                    android:text="UNIT"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_UNIT"
                    android:text="SET"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

            <!-- LIST_PRICE -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_LIST_PRICE"
                    android:text="LIST_PRICE"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_LIST_PRICE"
                    android:text="10000"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

            <!-- SALE_PERCENT -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_SALE_PERCENT"
                    android:text="SALE_PERCENT"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_SALE_PERCENT"
                    android:text="100"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

            <!-- UNIT_PRICE -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_UNIT_PRICE"
                    android:text="UNIT_PRICE"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_UNIT_PRICE"
                    android:text="10000"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

            <!-- TOTAL -->
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                >

                <TextView
                    android:id="@+id/lb_TOTAL"
                    android:text="TOTAL"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="right|center_vertical"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/blue"
                    android:background="@color/transparent"
                    />

                <EditText
                    android:id="@+id/ed_TOTAL"
                    android:text="20000"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="2"
                    android:layout_marginRight="6dp"
                    android:layout_marginLeft="6dp"
                    android:gravity="left|center_vertical"
                    android:background="@color/transparent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/black"
                    android:layout_column="2"

                    />

            </TableRow>

        </TableLayout>

        <ImageView
            android:id="@+id/cust_detail_bottom_icon"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scaleType="fitXY"
            android:layout_marginBottom="25dp"
            android:src="@drawable/customer_detail_bottom_1" />
        <!-- tb1 End -->

    </LinearLayout>


</RelativeLayout>
